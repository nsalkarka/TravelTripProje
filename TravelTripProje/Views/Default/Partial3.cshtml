@model List<TravelTripProje.Models.Siniflar.Blog>

<div class="padding">
    <h4>En popüler 10 blog</h4>
    <ul class="list list-pad">
        @{int i = 0; }
        @foreach (var item in Model)
        {


        <li class="list-bg"><span>@{i++;}@i.</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm blogları görüntüle</a>
</div>
